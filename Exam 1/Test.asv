y = linspace(-2,2);
[X,Y] = meshgrid(x,y);
temp = zero(length(x),length(y));
for i = 1:length(x)
        for j = 1:length(y)
            temp(i,j) = [x(i,j) y(i,j)]';
            est_r = distance(temp,land_mark);
        end
